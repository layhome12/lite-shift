generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id         Int       @id @default(autoincrement())
  username   String    @db.VarChar(50)
  password   String
  user_email String    @db.VarChar(150)
  user_nama  String    @db.VarChar(150)
  user_img   String?
  created_at DateTime? @default(now())
  updated_at DateTime?
}
